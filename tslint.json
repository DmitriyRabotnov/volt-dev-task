{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "packages"
    ]
  },
  "include": [
    "src/@types/*"
  ],
  "rules": {
    "interface-name": false
  }
}
